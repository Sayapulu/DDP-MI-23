#include <iostream>
using namespace std;

      int main() {
    int bermain;
    cin >> bermain;
    
    if (bermain < 0) {
        cout << "Negatif" << endl;
        
    }else if (bermain %2 == 0){
        cout << "Positif Genap" << endl;
        
    } else if ( bermain % 2 == 1){
        cout << "Positif Ganjil" << endl;
    }
        
         return 0;
}